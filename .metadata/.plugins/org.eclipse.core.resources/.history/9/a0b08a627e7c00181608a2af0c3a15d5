package com.qa.MongoDB;

public class Person {

	private int UserID;
	private String name;
	private String address;
	private String city;
	private String postcode;

	public Person(int userID, String name, String address, String city, String postcode) {
		super();
		UserID = userID;
		this.name = name;
		this.address = address;
		this.city = city;
		this.postcode = postcode;
	}

}
